/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    // Put your code here:
    Not16(in=a, out=nota16);
    Not16(in=b, out=notb16);

    And16(a=nota16, b=notb16, out=temp16);
    Not16(in=temp16, out=out);
}